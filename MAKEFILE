tst_rast: tst_all.o raster.o bitmap.o events.o model.o renderer.o rast_asm.o
	cc68x -g tst_all.o raster.o bitmap.o events.o model.o renderer.o rast_asm.o -o tst_all

tst_all.o: tst_all.c raster.h events.h
	cc68x -g -c tst_all.c

events.o: events.c events.h model.h
	cc68x -g -c events.c

raster.o: raster.c raster.h
	cc68x -g -c raster.c

model.o: model.c model.h
	cc68x -g -c model.c

bitmap.o: bitmap.c raster.h
	cc68x -g -c bitmap.c

renderer.o: renderer.c renderer.h
	cc68x -g -c renderer.c

rast_asm.o: rast_asm.s
	gen -D -L2 rast_asm.s
